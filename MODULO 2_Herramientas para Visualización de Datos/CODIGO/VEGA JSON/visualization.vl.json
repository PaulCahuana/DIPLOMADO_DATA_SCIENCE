{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "url": "data/seattle-weather.csv"
  },
  "mark": "area",
  "encoding": {
    "y": {
      "field": "temp_max",
      "aggregate": "average",
      "type": "quantitative"
    },
    "x": {
      "field": "date",
      "type": "temporal",
      "timeUnit": "month"
    }
  },
  "config": {}
}